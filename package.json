{
  "name": "ar-blacklist",
  "version": "0.0.1",
  "description": "Server that creates & maintains a strict adult content filter list.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "echo TRY RUNNING docker compose up --build -d",
    "pm2": "pm2 start ecosystem.config.js",
    "rater": "pm2 start ecosystem.config.js --only rater && pm2 logs",
    "dev": "pm2 start ecosystem.config.js --env development",
    "stop": "pm2 stop ecosystem.config.js",
    "docker:up": "docker-compose up --build -d && docker-compose logs -f",
    "docker:down": "docker-compose down --remove-orphans",
    "logs": "docker-compose logs -f --tail=15",
    "test:vid": "mocha -r ts-node/register tests/video-*.spec.ts",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts"
  },
  "keywords": [],
  "author": "Ros McMahon <ros@arweave.org>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/express": "^4.17.11",
    "@types/ffmpeg": "^1.0.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/sinon": "^10.0.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "mocha": "^8.3.2",
    "pm2": "^5.1.0",
    "sinon": "^10.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^3.4.0",
    "ansi-colors": "^4.1.1",
    "ar-gql": "0.0.6",
    "axios": "^0.21.1",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "ffmpeg": "^0.0.4",
    "file-type": "^16.5.0",
    "knex": "^0.95.4",
    "knex-utils": "^4.1.9",
    "nsfwjs": "^2.4.0",
    "pg": "^8.6.0"
  }
}
