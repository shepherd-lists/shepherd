{
  "name": "shepherd",
  "version": "0.1.1",
  "description": "Server that creates & maintains a strict adult content filter list.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "docker-compose up --build -d",
    "logs": "docker-compose logs -f --tail=15",
    "up": "docker-compose up --build -d && docker-compose logs -f",
    "down": "docker-compose down --remove-orphans",
    "rebuild": "npm run down && npm run up",
    "docker:psql": "docker exec -it shepherd_pgdb_1 psql -U postgres arblacklist",
    "test:grep": "mocha -r ts-node/register tests/**/*.spec.ts --grep",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "postversion": "git push && git push --tags"
  },
  "keywords": [],
  "author": "Ros McMahon <ros@arweave.org>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shepherd-media-classifier/shepherd.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.6",
    "chai": "^4.3.4",
    "mocha": "^9.0.2",
    "sinon": "^12.0.1"
  },
  "dependencies": {
    "@slack/webhook": "^6.0.0",
    "@types/express": "^4.17.11",
    "@types/node": "^16.11.9",
    "@types/rimraf": "^3.0.1",
    "@types/shelljs": "^0.8.9",
    "ansi-colors": "^4.1.1",
    "ar-gql": "0.0.6",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "file-type": "^16.5.3",
    "knex": "^0.95.14",
    "knex-utils": "^5.3.2",
    "pg": "^8.6.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
