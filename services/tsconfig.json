{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "noEmit": true,
    // "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    // "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "dist"],
  "ts-node": {
    "transpileOnly": true,
  }
}
